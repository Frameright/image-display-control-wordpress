{
  "dependencies": {
    "@prettier/plugin-php": "^0.19.1",
    "prettier": "^2.7.1",
    "markdown-spellcheck": "^1.3.1",
    "markdown-toc": "^1.2.0"
  },
  "scripts": {
    "format": "npx prettier --write .",
    "gentoc": "./bin/gentoc.sh",
    "spellcheck": "npx mdspell '**/*.md' 'readme.txt' '!**/node_modules/**' '!**/vendor/**' '!wpcs/**' --ignore-acronyms --ignore-numbers --no-suggestions --en-us"
  }
}
